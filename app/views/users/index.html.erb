<h1>All Users</h1>
<table class="table table-striped table-hover">
   <thead>
      <tr>
          <th>User ID</th>
          <th>name</th>
          <th>email</th>
        <th>date last sign in</th>
          <th>number of signin</th>


      </tr>
   </thead>


   <tbody>
      <tr>
        <% @users.each do |user| %>
            <td><%= user.id%></td>
           <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.last_sign_in_at %></td>
            <td><%= user.sign_in_count %></td>
            <% end %>
      </tr>
   </tbody>
</table>
